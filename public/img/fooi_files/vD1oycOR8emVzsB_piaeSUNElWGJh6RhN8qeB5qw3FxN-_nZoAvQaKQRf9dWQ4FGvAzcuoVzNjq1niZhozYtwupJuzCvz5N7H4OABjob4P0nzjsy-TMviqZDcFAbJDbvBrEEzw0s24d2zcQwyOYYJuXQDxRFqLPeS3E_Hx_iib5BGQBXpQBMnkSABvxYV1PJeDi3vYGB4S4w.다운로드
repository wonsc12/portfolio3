  const prevContext = shared.usePrev(context);
  const hasContext = context !== prevContext && hasProps(context);
  shared.useLayoutEffect(() => {
    if (hasContext) {
      shared.each(transitions, t => {
        t.ctrl.start({
          default: context
        });
      });
    }
  }, [context]);
  shared.each(changes, (_, t) => {
